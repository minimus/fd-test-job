{
  "globals": {
    "fetch": false,
    "URL": false,
    "window": false,
    "document": false,
    "Request": false,
    "Headers": false
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "func-names": ["error", "never"],
    "semi": ["error", "never"],
    "strict": 0,
    "babel/new-cap": 0,
    "babel/no-invalid-this": 1,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "plugins": [
    "babel",
    "react-hooks"
  ]
}
